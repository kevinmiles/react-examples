{
  "name": "redux-demo",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.5",
    "react-scripts": "^1.0.10",
    "redux": "^3.7.1"
  },
  "scripts": {
    "build": "npm build-css && react-scripts build",
