{
  "name": "redux-demo",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.5",
    "react-scripts": "^1.0.10",
    "redux": "^3.7.1"
  },
  "scripts": {
    "build": "npm build-css && react-scripts build",
    "build-css": "node-sass src/ -o src/",
    "flow": "flow",
    "format": "prettier-eslint --no-bracket-spacing --single-quote --write src/**/*.js",
    "lint": "eslint --quiet src --ext .js",
    "prepush": "npm run verify",
    "verify": "npm-run-all lint flow format test",
    "start": "npm-run-all -p watch-css react-start",
    "react-start": "react-scripts start",
    "test": "react-scripts test --env=jsdom",
    "watch-css": "npm run build-css && node-sass src/ -o src/ --watch"
  },
  "devDependencies": {
    "enzyme": "^2.9.1",
    "eslint": "^4.2.0",
    "eslint-plugin-flowtype": "^2.35.0",
    "eslint-plugin-html": "^3.1.0",
    "eslint-plugin-react": "^7.1.0",
    "flow-bin": "^0.49.1",
    "husky": "^0.14.3",
    "node-sass": "^4.5.3",
    "npm-run-all": "^4.0.2",
    "prettier-eslint-cli": "^4.1.1",
    "react-test-renderer": "^15.6.1",
    "redux-mock-store": "^1.2.3"
  }
}
